<body>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Phone</th>
            <th scope="col">City</th>
            <th scope="col">Postcode</th>
          </tr>
        </thead>
        <tbody>
            <% for(let i = 0; i < result.length; i++) { %> 
                <tr>
                    <td><%=result[i].name %> </td>
                    <td><%=result[i].email %> </td>
                    <td><%=result[i].number %> </td>
                    <td><%=result[i].city %> </td>
                    <td><%=result[i].zip %> </td>
                </tr>
                <% } %> 
